<h1>Listing request_skills</h1>

<table>
  <tr>
    <th>Skill</th>
    <th>Request</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @request_skills.each do |request_skill| %>
  <tr>
    <td><%= request_skill.skill_id %></td>
    <td><%= request_skill.request_id %></td>
    <td><%= link_to 'Show', request_skill %></td>
    <td><%= link_to 'Edit', edit_request_skill_path(request_skill) %></td>
    <td><%= link_to 'Destroy', request_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

